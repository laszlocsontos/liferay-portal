Bundle-Activator: com.liferay.portal.cxf.common.activators.CleanupActivator
Bundle-ClassPath:\
	.,\
	lib/cxf-rt-frontend-jaxrs.jar,\
	lib/cxf-rt-rs-extension-providers.jar,\
	lib/javax.annotation-api.jar,\
	lib/javax.json-api.jar,\
	lib/javax.ws.rs-api.jar,\
	lib/jettison.jar
Bundle-Name: Liferay Portal CXF common JAXRS infrastructure
Bundle-SymbolicName: com.liferay.portal.cxf.jaxrs.common
Bundle-Version: 1.0.0
Export-Package:\
	javax.ws.rs.*,\
	javax.json.*,\
	org.apache.cxf.*;version="3.0.3"
Import-Package:\
	!com.sun.*,\
	!org.springframework.*,\
	!javax.validation.*,\
	!org.apache.abdera.*,\
	!org.apache.aries.*,\
	!org.apache.cxf.aegis.*,\
	!org.apache.cxf.ws.policy.*,\
	!org.apache.neethi.*,\
	!org.apache.velocity.*,\
	!org.apache.xml.resolver.*,\
	!org.apache.xmlbeans.*,\
	!org.jvnet.fastinfoset.*,\
	!org.jvnet.staxex.*,\
	!org.junit.*,\
	!org.relaxng.datatype.*,\
	!org.slf4j.spi.*,\
	!net.sf.cglib.proxy.*,\
	*
Include-Resource:\
	classes,\
	lib/=lib/cxf-rt-frontend-jaxrs.jar,\
	lib/=lib/cxf-rt-rs-extension-providers.jar,\
	lib/=lib/javax.annotation-api.jar,\
	lib/=lib/javax.json-api.jar,\
	lib/=lib/javax.ws.rs-api.jar,\
	lib/=lib/jettison.jar